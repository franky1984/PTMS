@{
    ViewBag.Title = "黑名单管理";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap">
    <div class="col-xs-12 lr-form-item"  data-table="F_Base_BlackList" >
        <div class="lr-form-item-title">用人单位</div>
        <input id="F_EmployerName" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="F_Base_BlackList" >
        <div class="lr-form-item-title">工种</div>
        <input id="F_CategoryName" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="F_Base_BlackList" >
        <div class="lr-form-item-title">姓名</div>
        <input id="F_RealName" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="F_Base_BlackList" >
        <div class="lr-form-item-title">原因</div>
        <input id="F_Cause" type="text" class="form-control"  />
    </div>
</div>
@Html.AppendJsFile("/Areas/LR_CodeDemo/Views/Black/Form.js")
